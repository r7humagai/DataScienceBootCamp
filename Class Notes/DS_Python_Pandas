        "# Functions that are very frequently used in series\n",
        "# Same set of functions are applicable in dataframes\n",
        "#. Note: Series - 1-Dimensional Data\n",
        "#.       DataFrames - Multi-Dimensional Data"
      ],
      "metadata": {
        "id": "HpcG8YPwSJPD"
      },
      "execution_count": 24,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "lst1 =[12,23,34,56,78,90]\n",
        "\n",
        "sr1 = pd.Series(lst1)\n",
        "print('')\n",
        "print('\\n1*: \\n',sr1, len(sr1))\n",
        "print('\\n2*: \\n',sr1, sr1.count())\n",
        "print('\\n3*: \\n',sr1.ndim)\n",
        "print('\\n4*: \\n',type(sr1))\n",
        "print('\\n5*: \\n',sr1.size)\n",
        "print('\\n6*: \\n',sr1.dtype)\n",
        "print('\\n7*: \\n',sr1.item)\n",
        "print('\\n8*: \\n',sr1.items)\n",
        "print('\\n9*: \\n',sr1.nbytes)\n",
        "print('\\n10*: \\n',sr1.shape)"
      ],
