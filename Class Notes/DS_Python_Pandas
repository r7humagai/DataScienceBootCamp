    {
      "cell_type": "code",
      "source": [
        "# add percentage sign to column ' Vehicle_Theft_Per_Population'\n",
        "\n",
        "newdataFrame = df_uscrime[['Year', 'Population', 'Vehicle_Theft', 'Murder']].iloc[44:55,:]\n",
        "newdataFrame['Vehicle_Theft_Per_Population'] = newdataFrame['Vehicle_Theft'] / newdataFrame['Population']\n",
        "newdataFrame['Murder_Per_Population'] = newdataFrame['Murder'] / newdataFrame['Population']\n",
        "newdataFrame\n",
        "print('before changing to %', newdataFrame.dtypes)\n",
        "\n",
        "after_percentage_sign = []      #empty array that should contain the string value\n",
        "for datas in newdataFrame['Vehicle_Theft_Per_Population']:    # This for loop will convert the integer values\n",
        "  add_percentage = \"{:.2%}\".format(datas)\n",
        "  after_percentage_sign.append(add_percentage)\n",
        "\n",
        "print(type(after_percentage_sign[3]))\n",
        "\n",
        "\n",
        "newdataFrame['Vehicle_Theft_Per_Population'] = after_percentage_sign\n",
        "print('\\nafter changing to %',newdataFrame.dtypes)\n",
        "newdataFrame\n"
      ],
