        "list_to_change_murder_per_population = []       # Empty list to store the string data after conversion\n",
        "\n",
        "for datas in newdataFrame['Murder_Per_Population']:    # loop through each data\n",
        "  add_percent_sign = \"{:.4%}\".format(datas)            # format it to make it string in 4 digits with percentage sign\n",
        "  list_to_change_murder_per_population.append(add_percent_sign)   # the list gets all the new data\n",
        "\n",
        "newdataFrame['Murder_Per_Population'] = list_to_change_murder_per_population    # change the data of 'Murder_Per_Population' to new data\n",
