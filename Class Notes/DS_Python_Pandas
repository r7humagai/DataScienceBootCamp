        "df_ChurnModelling[(df_ChurnModelling.Geography != 'Spain')&\n",
        "                   (df_ChurnModelling.Age >= 70)&\n",
        "                   (df_ChurnModelling.Tenure > 6)&\n",
        "                   (df_ChurnModelling.NumOfProducts != 2)&\n",
        "                   (df_ChurnModelling.CreditScore > 700)&\n",
        "                   (df_ChurnModelling.IsActiveMember != 1)\n",
        "                  ]\n",
        "\n",
        "# This query produces all the customers\n",
        "#(Who are not from Spain) ,\n",
        "#(Who are 45 years or less),\n",
        "#( Who have a tenure of more than 6 years),\n",
        "#(Who don't have numofProducts exactly 2),\n",
        "#(Who have credit scores more than 700 ),\n",
        "#(Is not an active member)"
