        "# Using Drop Method to delete a certain column from df1 ande save it to df2\n",
        "\n",
        "dict1 = {'A': pd.Series([1,2,3,4], index = ['a','b','c','d']),\n",
        "         'B': pd.Series([1,2,3,4,5], index = ['a','b','c','d','e'])\n",
        "         }\n",
        "\n",
        "df1 = pd.DataFrame(dict1)\n",
        "df2 = df1.drop('A',axis = 1)\n",
        "df2"
