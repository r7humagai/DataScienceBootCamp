      "cell_type": "code",
      "source": [
        "# Make a new column to put data of 'Murder_theft_per_population'\n",
        "#' newdataFrame ' is the new data frame to contain only the required values\n",
        "\n",
        "newdataFrame = df_uscrime[['Year', 'Population', 'Vehicle_Theft', 'Murder']].iloc[44:55,:]\n",
        "newdataFrame['Vehicle_Theft_Per_Population'] = newdataFrame['Vehicle_Theft'] / newdataFrame['Population']\n",
        "newdataFrame['Murder_Per_Population'] = newdataFrame['Murder'] / newdataFrame['Population']\n",
        "newdataFrame\n",
        "\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
